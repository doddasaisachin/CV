    "from deepface import DeepFace\n",
    "from google.colab.patches import cv2_imshow\n",
    "image=cv2.imread('emotion.jpg')\n",
    "emotion=DeepFace.analyze(image,actions=['emotion'])\n",
    "cv2_imshow(image)\n",
    "print(emotion)"
